<div class="gl_header"> 
  <div class="links">
    <%= link_to "Advisors Mgmt", advisors_mgmt_path %>
    <%= link_to "Profiles Mgmt", profiles_mgmt_path %>
    <%= link_to "Listings Mgmt", listings_mgmt_path %>
  </div>
</div>

<div class="simple_container">  
  <h2>Admin Area - Profiles Mgmt</h2> <br>
  <% @all_profiles.each do |pro| %>
    <% if pro.advisor %>
      <h4> Profile by <%= pro.advisor.first_name %> <%= pro.advisor.second_name %> </h4>
    <% end %> 
    <p> <%= pro.description %> </p>
    <p> <%= link_to "Edit |", "#" %> <%= link_to "Delete", "#" %> </p> <br> <br>
  <% end %>
</div>
